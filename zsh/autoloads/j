# jump around like z
unfunction j

case $(uname) in
  Linux)
    [[ -s /home/torgeir/.autojump/etc/profile.d/autojump.sh ]] && source /home/torgeir/.autojump/etc/profile.d/autojump.sh
    ;;
  Darwin)
    [[ -s /usr/local/share/autojump/autojump.zsh ]] && source /usr/local/share/autojump/autojump.zsh
    ;;
esac

j "$@"
